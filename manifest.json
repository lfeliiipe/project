{
    "name": "Focus Helper and Study Logger",
    "description": "Extension to help you studying/working",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "oauth2": {
      "client_id": "144113858899-uc9f84basj6124a7gs1239ba4tiv1i3d.apps.googleusercontent.com",
        "scopes":["openid email profile"]
    },
    "permissions": ["alarms", "identity", "identity.email", "notifications", "storage", "scripting", "webNavigation"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": [ "block.html", "zone_page.html" ],
        "matches": [ "<all_urls>" ]
      }
    ]
}
  